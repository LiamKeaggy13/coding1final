$("#button").click(function(){
var userChoiceMetal = $("#metal").val();
var userChoiceLeather = $("#leather").val();
var userChoiceColor = $("#color").val();


     if(userChoiceMetal=="silver" && userChoiceLeather=="smooth" && userChoiceColor=="tan"){
  choice1()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="smooth" && userChoiceColor=="brown"){
   choice2()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="smooth" && userChoiceColor=="black"){
   choice3()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="alligator" && userChoiceColor=="tan"){
   choice4()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="alligator" && userChoiceColor=="brown"){
   choice5()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="alligator" && userChoiceColor=="black"){
   choice6()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="grained" && userChoiceColor=="tan"){
   choice7()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="grained" && userChoiceColor=="brown"){
   choice8()
}
else if(userChoiceMetal=="silver" && userChoiceLeather=="grained" && userChoiceColor=="black"){
   choice9()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="smooth" && userChoiceColor=="tan"){
   choice10()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="smooth" && userChoiceColor=="brown"){
   choice11()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="smooth" && userChoiceColor=="black"){
   choice12()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="alligator" && userChoiceColor=="tan"){
   choice13()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="alligator" && userChoiceColor=="brown"){
   choice14()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="alligator" && userChoiceColor=="black"){
   choice15()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="grained" && userChoiceColor=="tan"){
   choice16()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="grained" && userChoiceColor=="brown"){
   choice17()
}
else if(userChoiceMetal=="gold" && userChoiceLeather=="grained" && userChoiceColor=="black"){
   choice18()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="smooth" && userChoiceColor=="tan"){
   choice19()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="smooth" && userChoiceColor=="brown"){
   choice20()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="smooth" && userChoiceColor=="black"){
   choice21()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="alligator" && userChoiceColor=="tan"){
   choice22()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="alligator" && userChoiceColor=="brown"){
   choice23()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="alligator" && userChoiceColor=="black"){
   choice24()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="grained" && userChoiceColor=="tan"){
   choice25()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="grained" && userChoiceColor=="brown"){
   choice26()
}
else if(userChoiceMetal=="black" && userChoiceLeather=="grained" && userChoiceColor=="black"){
   choice27()
}
else{
  alert("Please pick all three categories! That way we can calculate the best match for you.")
}
});



$("#button1").click(function(){
  $("#beltsil").attr("src", "http://silhouettesfree.com/animals/domestic-animals/belt-and-buckle-silhouette-image.png")
  $("#shoesil").attr("src", "https://image.flaticon.com/icons/png/512/44/44307.png")
  $("#watchsil").attr("src", "https://image.flaticon.com/icons/svg/33/33472.svg")
  $("#metal").each(function() { this.selectedIndex = 0 })
  $("#leather").each(function() { this.selectedIndex = 0 })
  $("#color").each(function() { this.selectedIndex = 0 })
})
